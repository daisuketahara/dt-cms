{% extends templateservice.getTemplate() %}

{% block content %}
    {{content|raw}}
{% endblock %}

{% block meta %}
    {{ parent() }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <style>
    {% if custom_css %}
    {{custom_css|raw}}
    {% endif %}
    </style>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% if custom_js %}
    <script type="text/javascript">
    {{custom_js}}
    </script>
    {% endif %}
{% endblock %}
